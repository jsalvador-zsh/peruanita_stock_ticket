<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_picking_form_inherit_ticket" model="ir.ui.view">
        <field name="name">stock.picking.form.inherit.ticket</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button name="btn_ticket" string="Imprimir Ticket" icon="fa-print" type="object" class="btn-primary"/>
            </xpath>
        </field>
    </record>
</odoo>